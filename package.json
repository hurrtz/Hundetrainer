    "react-native-gesture-handler": "^1.1.0",
    "react-native-vector-icons": "^6.4.1",
    "react-navigation": "^3.4.1",
    "react-navigation-header-buttons": "^2.1.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
